{
  "name": "svelte-auth",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "migrate": "bun run migrate:generate && bun run migrate:run",
    "migrate:generate": "bunx drizzle-kit generate:sqlite",
    "migrate:run": "bun run src/lib/script/migrate.ts"
  },
  "devDependencies": {
    "@sveltejs/adapter-node": "^4.0.1",
    "@sveltejs/kit": "^2.5.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.2",
    "@types/better-sqlite3": "^7.6.9",
    "@types/node": "^20.11.17",
    "@types/nodemailer": "^6.4.14",
    "@types/pg": "^8.11.0",
    "drizzle-kit": "^0.20.14",
    "svelte": "^4.2.10",
    "svelte-check": "^3.6.4",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.1.1"
  },
  "type": "module",
  "dependencies": {
    "@catdadcode/svelte-adapter-bun": "^0.5.7",
    "@lucia-auth/adapter-sqlite": "^3.0.0",
    "@vinejs/vine": "^1.7.1",
    "arctic": "^1.1.4",
    "better-sqlite3": "^9.4.1",
    "drizzle-orm": "^0.29.3",
    "lucia": "^3.0.1",
    "nodemailer": "^6.9.9",
    "oslo": "^1.1.0",
    "pg": "^8.11.3",
    "svelte-sonner": "^0.3.17",
    "unique-username-generator": "^1.3.0"
  }
}
